    <title>Document</title>
    <script>
      if(window.location.href!='http://localhost:3000/dashboard'){
        window.onload = function() {
            var home = document.getElementById("home")
            var addproject = document.getElementById("addproject")
            var projectdetails = document.getElementById("projectdetails")
            var taskdetails = document.getElementById("taskdetails")
            var dashboard=document.getElementById("dashboardcontent")
            var loginrequest=document.getElementById("loginrequest");
            var loc = window.location.href;
             
            if(loc=='http://localhost:3000/') {
                home.style.color='white';
            } else if(loc=='http://localhost:3000/manager') {
                addproject.style.color='white';
            } else if(loc=='http://localhost:3000/projectDetails'){
                projectdetails.style.color='white';
            }else if(loc=='http://localhost:3000/dashboard'){
                dashboard.style.color='white';
            }else if(loc=='http://localhost:3000/loginrequest'){
              loginrequest.style.color='white';
            } else {
                taskdetails.style.color='white';
            }

        }
      }
        
        function navbar(){
          var formelement = document.querySelector("#formelement");
          formelement.action = window.location.href+'/notification';
          document.getElementById("notificationlength").innerHTML=0;
          setTimeout( function(){document.getElementById("formelement").submit()},3000)


        }

    </script>
  
