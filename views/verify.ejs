<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>AdminLTE 3 | General Form Elements</title>

  
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
  
  <link rel="stylesheet" href="/stylesheets/all.min.css">
 
  <link rel="stylesheet" href="/stylesheets/adminlte.min.css">
  <!-- <script src="/javascripts/form.js"></script> -->
</head>
<body class="hold-transition sidebar-mini">
<div class="wrapper">
  
    <%- include('./partials/navbar.ejs') %>

 

  <div class="content-wrapper" style="margin-left:0px">
    
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <!-- <h1>General Form</h1> -->
          </div>
          <div class="col-sm-6">
            <!-- <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="#">Home</a></li>
              <li class="breadcrumb-item active">General Form</li>
            </ol> -->
          </div>
        </div>
      </div>
    </section>

   
    <section class="content">
      <div class="container-fluid">
        <div class="row">
          <div class="col-md-3"></div>
          <div class="col-md-6">
            <!-- general form elements -->
            <div class="card card-primary">
              <div class="card-header">
                <h3 class="card-title">Check Your email</h3>
              </div>
              <!-- /.card-header -->
              <!-- form start -->
              <form  action="/registercodevalidate" method="POST">
                <div class="card-body">
                    <div class="form-group">
                        <label for="exampleInputno">Verification code<span style="color: red;">&nbsp;*</span></label>
                        <input  type="text" name="verifycode" class="form-control" id="verifycode" placeholder="Enter verifycode" required>
                        
                      </div>
                     
                      <div class="form-group">
                        <!-- <label for="exampleInputno">Verification code<span style="color: red;">&nbsp;*</span></label> -->
                        <!-- <%=id1%>
                        <%console.log(id1)%> -->
                        <input  type="hidden" name="idnum" class="form-control" id="verifycode" placeholder="Enter verifycode" value="<%=id1 %>" required>
                        
                      </div>
                
                </div>
                
                <div class="card-footer" style="text-align: center;">
                  <button type="submit"  class="btn btn-primary"><b>Submit</b></button>
                </div>
              </form>
              
            </div>
        

          </div>
          <div class="col-md-3"></div>
          
        
        </div>
    
      </div>
    </section>
   
  </div>
  
  <footer class="main-footer">
    <div class="float-right d-none d-sm-block">
      <b>Version</b> 3.2.0
    </div>
